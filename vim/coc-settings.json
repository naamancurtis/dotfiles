{
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "eslint.autoFixOnSave": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.messageTarget": "echo",
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.checkOnSave.allFeatures": true,
  "rust-analyzer.hoverActions.enable": true,
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.completion.postfix.enable": false,
  "rust-analyzer.lens.methodReferences": true,
  "rust-analyzer.hover.linksInHover": true,
  "rust-analyzer.assist.importGranularity": "module",
  "rust-analyzer.cargo.runBuildScripts": true,
  "rust-analyzer.assist.importEnforceGranularity": true,
  "rust-analyzer.experimental.procAttrMacros": true,
  "rust-analyzer.debug.runtime": "vimspector",
  "rust-analyzer.completion.autoimport.enable": true,
  "rust-analyzer.lruCapacity": 256,
  "rust-analyzer.cargo.loadOutDirsFromCheck": true,
  "tsserver.implicitProjectConfig.experimentalDecorators": true,
  "languageserver": {
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform"],
      "initializationOptions": {}
    }
  }
}
